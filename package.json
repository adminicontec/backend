{
  "name": "campus_virtual_icontec",
  "version": "1.0.0",
  "main": "./src/app/server.js",
  "bin": "./src/client/client.ts",
  "scripts": {
    "init": "npm i -g",
    "start": "node ./src/app/server.js",
    "pm2_stop": "pm2 stop all",
